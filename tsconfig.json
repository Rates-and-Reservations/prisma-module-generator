{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "outDir": "./bin",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
